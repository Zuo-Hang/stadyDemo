<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="student" class="com.example.demo.spring.Student">
        <property name="name" value="zhangsan" />
    </bean>

    <bean id="school" class="com.example.demo.spring.School">
    </bean>

    <bean id="factoryBeanPojo" class="com.example.demo.spring.FactoryBeanPojo">
        <property name="type" value="student" />
    </bean>

</beans>